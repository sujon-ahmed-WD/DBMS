use dummydb;
 select EMPLOYEES.FIRST_NAME,DEPARTMENTS.DEPARTMENT_NAME --  DPARTMENT_ID ... 
 FROM EMPLOYEES ,DEPARTMENTS
 WHERE EMPLOYEES.DEPARTMENT_ID =DEPARTMENTS.DEPARTMENT_ID;
 
SELECT  EMPLOYEES.FIRST_NAME  ,DEPARTMENTS.DEPARTMENT_NAME  -- JOIN METHOND 1
FROM EMPLOYEES JOIN DEPARTMENTS ON EMPLOYEES.DEPARTMENT_ID=DEPARTMENTS.DEPARTMENT_ID;

SELECT  EMPLOYEES.FIRST_NAME  ,DEPARTMENTS.DEPARTMENT_NAME  -- JOIN METHOD 2 ... 
FROM EMPLOYEES JOIN DEPARTMENTS USING(DEPARTMENT_ID);


SELECT EMPLOYEES.FIRST_NAME,DEPARTMENTS.DEPARTMENT_NAME FROM EMPLOYEES JOIN DEPARTMENT ON EMPLOYEES.DEPARTMENT_ID=DEPARTMENT.DEPARTMENT_ID
