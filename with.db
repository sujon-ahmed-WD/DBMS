WITH AVGIT AS 
(SELECT AVG(SALARY) AS SAL1

FROM employees
WHERE DEPARTMENT_ID=60 -- IT 

),

MAXMKT AS 
(

	SELECT MAX(SALARY) AS SAL2
    FROM employees
    WHERE DEPARTMENT_ID =20 -- MERKETING 
)
SELECT *
FROM EMPLOYEES 
WHERE SALARY >(SELECT SAL1 FROM AVGIT) AND SALARY <(SELECT SAL2 FROM MAXMKT );







